    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2017": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "concurrently": "^1.0.0",
    "nodemon": "^1.4.1",
    "tap": "^1.4.0"
  },
  "dependencies": {
