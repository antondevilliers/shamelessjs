{
  "name": "shamelessjs",
  "version": "1.0.0",
  "description": "Module for parsing page contents of specific sources",
  "main": "build/index.js",
  "scripts": {
    "compile": "make build",
    "start": "node src/index.js",
    "watch": "make watch",
